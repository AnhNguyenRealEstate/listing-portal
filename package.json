{
  "name": "listing-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/listing-portal/stats.json",
    "build": "ng build --stats-json && gulp copy-index-html-to-cloud-functions",
    "build-functions": "npm run --prefix functions build",
    "build-all-prod": "ng build --stats-json --configuration production --aot --output-hashing=all && gulp copy-index-html-to-cloud-functions && npm run build-functions",
    "preview-live": "npm run build-all-prod && firebase hosting:channel:deploy listing-portal-uat && npm run deploy-functions",
    "deploy-functions": "npm run --prefix functions deploy",
    "deploy-hosting": "npm run build-all-prod && firebase deploy --only hosting",
    "deploy-full": "npm run deploy-hosting && npm run deploy-functions",
    "build-docker": "docker build -t listing-portal:latest \".\" ",
    "start-docker": "docker run -dp 3000:80 --name listing-portal -t listing-portal:latest",
    "start-db": "firebase emulators:start --import=./offline-db --export-on-exit",
    "clean-db": "lsof -t -i:9199 -i:8080 -i:9099 -i:5001 -i:9000 -i:5050 -i:8085 -i:4000 | xargs kill -9"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.0.4",
    "@angular/google-maps": "^15.1.0",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@ng-idle/core": "*",
    "@ng-idle/keepalive": "*",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@tinymce/tinymce-angular": "^7.0.0",
    "autoprefixer": "10.4.9",
    "bootstrap": "^5.2.2",
    "merge-images": "^2.0.0",
    "ngx-image-compress": "^13.1.13",
    "ngx-mask": "^14.0.3",
    "ngx-translate": "github:ngx-translate/core",
    "ngx-trim-directive": "^2.0.0",
    "ngx-useful-swiper": "^10.0.1",
    "placeholder-loading": "^0.6.0",
    "rxjs": "~7.5.5",
    "swiper": "^6.8.4",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "^15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@angular/localize": "^15.0.4",
    "@types/jasmine": "~4.3.0",
    "@types/merge-images": "^1.2.1",
    "@types/node": "^18.7.18",
    "angular-cli-ghpages": "*",
    "codelyzer": "^6.0.0",
    "gulp": "^4.0.2",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-image-slider": "^7.0.0",
    "ngx-page-scroll-core": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.1",
    "tslint": "~6.1.0",
    "typescript": "^4.8.4",
    "webpack-bundle-analyzer": "^4.6.1"
  }
}
